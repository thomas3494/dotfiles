alias w3m='w3m duckduckgo.com'
alias xclip='xclip -selection clipboard'
alias dmount="udisksctl mount -b"
alias dumount="udisksctl unmount -b"
alias mpvi="mpv --loop-file=inf"

# https://www.kernel.org/doc/Documentation/cpu-freq/governors.txt
alias powersave='echo "powersave" | sudo tee /sys/devices/system/cpu/cpu*/cpufreq/scaling_governor'
alias ondemand='echo "ondemand" | sudo tee /sys/devices/system/cpu/cpu*/cpufreq/scaling_governor'

alias config="git --git-dir=$HOME/.dotfiles --work-tree=$HOME"
